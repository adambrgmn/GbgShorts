{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "browser": true,
    "node": true,
    "shared-node-browser": true,
    "commonjs": true,
    "es6": true
  },
  "rules": {
    "no-console": 0,

    "react/jsx-filename-extension": 0
  }
}
